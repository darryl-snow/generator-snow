.icon
	display 		inline-block
	height 			1em
	vertical-align 	middle
	width 			1em
<% if(!svgicons) { %>
@font-face
	font-family 	"snow"
	src				url("fonts/snow.eot?r4pjdqq")
	src				url("fonts/snow.eot?#iefixr4pjdqq") format("embedded-opentype"),
					url("fonts/snow.woff?r4pjdqq") format("woff"),
					url("fonts/snow.ttf?r4pjdqq") format("truetype"),
					url("fonts/snow.svg?r4pjdqq#snow") format("svg")
	font-weight 	normal
	font-style 		normal

[class^="icon-"],
[class*=" icon-"]
	font-family		"snow"
	font-style		normal
	font-variant	normal
	font-weight		normal
	line-height		1
	margin 			0 0.2em
	speak			none
	text-transform	none
	vertical-align 	middle
	-webkit-font-smoothing	antialiased
	-moz-osx-font-smoothing	grayscale

.icon-user-add:before
	content			"\e600"

.icon-heart:before
	content			"\e601"

.icon-arrow-left:before
	content			"\e620"

.icon-arrow-down:before
	content			"\e603"

.icon-arrow-up:before
	content			"\e621"

.icon-arrow-right:before
	content			"\e602"<% } %>